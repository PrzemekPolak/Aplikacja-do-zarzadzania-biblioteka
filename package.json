{
  "name": "electron-app",
  "version": "1.0.0",
  "private": true,
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "create:db": "cross-env ELECTRON_RUN_AS_NODE=true ./node_modules/.bin/electron ./src/renderer/database/createDb.js",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "test": "cross-env ELECTRON_RUN_AS_NODE=true ./node_modules/.bin/electron ./node_modules/jest-cli/bin/jest.js --runInBand",
    "test:specific": "cross-env ELECTRON_RUN_AS_NODE=true ./node_modules/.bin/electron ./node_modules/jest-cli/bin/jest.js jest borrowings.test.ts --runInBand"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "better-sqlite3": "^9.3.0",
    "inversify": "^6.0.2",
    "reflect-metadata": "^0.2.1"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.10",
    "@types/better-sqlite3": "^7.6.10",
    "@types/node": "16.18.21",
    "@types/react": "18.0.30",
    "@types/react-datepicker": "^4.11.2",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-react": "^3.1.0",
    "cross-env": "^7.0.3",
    "electron": "22.3.4",
    "electron-builder": "23.6.0",
    "electron-rebuild": "^3.2.9",
    "electron-vite": "^1.0.21",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "formik": "^2.2.9",
    "jest": "^29.7.0",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-datepicker": "^4.11.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.8.2",
    "react-router-dom": "^6.8.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "vite": "^4.2.1",
    "yup": "^1.2.0"
  }
}
